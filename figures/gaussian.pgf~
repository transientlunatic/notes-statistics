\begin{tikzpicture}
    \begin{axis} [
    width=\linewidth, height=2in,
    xmin=0, xmax=20,
%   xtick={7700,7725,...,7800},
%   ymin=0.009, ymax=0.05,
    ]
    \addplot gnuplot[raw gnuplot, id=normal, mark=none, muted-orange, ultra thick]{
      set xrange[0:20];
      norm(sig, mu, x) = exp(- (x-mu)^2 / (2*sig^2) )/(sqrt(2*pi)*sig)
      plot besj(2,x);
    };
    \end{axis}
\end{tikzpicture}